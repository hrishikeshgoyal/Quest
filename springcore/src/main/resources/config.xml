<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="org.example.Student" name="student1">
        <property name="studentId" value="12345">
        </property>
        <property name="studentName">
            <value>Hrishikesh Goyal</value>
        </property>

        <property name="studentAddress">
            <value>Jodhpur</value>
        </property>

    </bean>

    <bean class="org.example.Student" name="student2" p:studentId="2345" p:studentName="Goyal" p:studentAddress="Rajasthan" />

</beans>